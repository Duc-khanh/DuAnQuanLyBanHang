<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.laptopthachthat.LoginController">
    <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0">
        <left>
            <AnchorPane prefHeight="541.0" prefWidth="503.0" styleClass="vbox_background_center" stylesheets="@login.css" BorderPane.alignment="CENTER">
            <children>
                      <Label layoutX="147.0" layoutY="410.0" prefHeight="67.0" prefWidth="271.0" text="MacBook" textFill="GOLD">
                          <font>
                              <Font name="Comic Sans MS" size="48.0" />
                          </font>
                      </Label>
                      <ImageView fitHeight="247.0" fitWidth="269.0" layoutX="110.0" layoutY="173.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@Image/pngtree-mummy-yellow-laptop-sticker-vector-png-image_12226763.png" />
                          </image>
                      </ImageView>
                      <Label layoutX="57.0" layoutY="88.0" prefHeight="71.0" prefWidth="393.0" text="LapTop Thach That">
                          <font>
                              <Font name="SignPainter-HouseScript" size="64.0" />
                          </font>
                          <effect>
                              <DropShadow blurType="ONE_PASS_BOX" color="WHITE" />
                          </effect>
                      </Label>
            </children>
            </AnchorPane>
        </left>
      <right>
         <AnchorPane prefHeight="600.0" prefWidth="410.0" BorderPane.alignment="CENTER">
            <children>
                      <TextField fx:id="username" layoutX="98.0" layoutY="266.0" prefHeight="36.0" prefWidth="198.0" promptText="Username" style="-fx-border-color: #ffa601; -fx-border-width: 0px 0px 2px 0px;" />
                    <Button fx:id="signInButton" layoutX="98.0" layoutY="368.0" mnemonicParsing="false" onAction="#signIn" prefHeight="43.0" prefWidth="173.0" style="-fx-background-color: #FFD700;" text="Sign in" textFill="WHITE" />


                    <Button fx:id="signUpButton" layoutX="99.0" layoutY="445.0" mnemonicParsing="false" onAction="#showSignUp" prefHeight="43.0" prefWidth="172.0" style="-fx-background-color: #FFD700;" text="Sign up" textFill="WHITE" />
                      <ImageView fitHeight="157.0" fitWidth="132.0" layoutX="128.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@Image/office-man.png" />
                          </image>
                      </ImageView>
                      <PasswordField fx:id="password" layoutX="95.0" layoutY="224.0" prefHeight="33.0" prefWidth="194.0" promptText="Password" style="-fx-border-color: #ffa601; -fx-border-width: 0px 0px 2px 0px;" />
                    <Text fill="#bc8640" layoutX="154.0" layoutY="189.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User login" wrappingWidth="83.30989456176758">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Text>
               <ImageView fitHeight="33.0" fitWidth="28.0" layoutX="60.0" layoutY="229.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@Image/9821477.png" />
                  </image>
               </ImageView>
               <ImageView fitHeight="31.0" fitWidth="29.0" layoutX="58.0" layoutY="273.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@Image/3917787.png" />
                  </image>
               </ImageView>
            </children>
         </AnchorPane>
      </right>
    </BorderPane>
</AnchorPane>
