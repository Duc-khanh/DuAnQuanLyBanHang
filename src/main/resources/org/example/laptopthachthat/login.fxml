<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<AnchorPane fx:controller="org.example.laptopthachthat.LoginController" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/8.0.171">
    <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
                prefWidth="900.0">
        <left>
            <AnchorPane prefHeight="800.0" prefWidth="503.0" styleClass="vbox_background_center"
                        stylesheets="@login.css"
                        BorderPane.alignment="CENTER">
                <children>
                    <VBox alignment="CENTER" prefHeight="800.0" prefWidth="503.0">
                        <children>
                            <Label prefHeight="51.0" prefWidth="244.0" text="LapTop Thach That">
                                <font>
                                    <Font name="SignPainter-HouseScript" size="28.0"/>
                                </font>
                                <effect>
                                    <DropShadow blurType="ONE_PASS_BOX" color="WHITE"/>
                                </effect>
                            </Label>
                            <ImageView fitHeight="192.0" fitWidth="232.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@Image/pngtree-mummy-yellow-laptop-sticker-vector-png-image_12226763.png"/>
                                </image>
                            </ImageView>
                            <Label text="MacBook" textFill="GOLD">
                                <font>
                                    <Font name="Comic Sans MS" size="36.0"/>
                                </font>
                            </Label>
                        </children>
                    </VBox>
                </children>
            </AnchorPane>
        </left>
        <center>
            <VBox alignment="CENTER" prefHeight="679.0" prefWidth="397.0" BorderPane.alignment="CENTER">
                <children>
                    <ImageView fitHeight="111.0" fitWidth="116.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@Image/images.jpeg"/>
                        </image>
                    </ImageView>
                    <Text fill="#bc8640" strokeType="OUTSIDE" strokeWidth="0.0" text="User login"
                          wrappingWidth="83.30989456176758">
                        <font>
                            <Font size="18.0"/>
                        </font>
                    </Text>
                    <HBox alignment="CENTER" prefHeight="53.0" prefWidth="777.0">
                        <children>
                            <ImageView fitHeight="24.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@Image/office-man.png"/>
                                </image>
                            </ImageView>
                            <TextField fx:id="username" prefHeight="29.0" prefWidth="169.0" promptText="Username"
                                       style="-fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;"/>
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" prefHeight="62.0" prefWidth="777.0">
                        <children>
                            <ImageView fitHeight="29.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@Image/Img.png"/>
                                </image>
                            </ImageView>
                            <PasswordField fx:id="password" prefHeight="29.0" prefWidth="169.0" promptText="Password"
                                           style="-fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;"/>
                        </children>
                    </HBox>
                    <ImageView fitHeight="23.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true"/>
                    <Button fx:id="signInButton" layoutX="64.0" layoutY="309.0" mnemonicParsing="false"
                            prefHeight="31.0"
                            prefWidth="140.0" style="-fx-background-color: #FFD700;" text="Sign in" textFill="WHITE"
                            onAction="#signIn"/>


                    <Button fx:id="signUpButton" layoutX="84.0" layoutY="450.0" mnemonicParsing="false"
                                prefHeight="31.0"
                                prefWidth="140.0" style="-fx-background-color: #FFD700;" text="Sign up" textFill="WHITE"
                            onAction="#showSignUp"/>

                </children>
            </VBox>
        </center>
    </BorderPane>
</AnchorPane>

