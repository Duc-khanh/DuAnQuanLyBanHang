<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="568.0" prefWidth="702.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.laptopthachthat.User.UserDisplay">

    <children>
        <TableView fx:id="userTable" layoutY="-1.0" prefHeight="513.0" prefWidth="702.0">
            <columns>
                <TableColumn fx:id="userIDColumn" prefWidth="66.0" text="ID" />
                <TableColumn fx:id="roleColumn" prefWidth="72.0" text="Role" />
                <TableColumn fx:id="usernameColumn" prefWidth="92.0" text="UserName" />
                <TableColumn fx:id="passwordColumn" prefWidth="111.0" text="Password" />
                <TableColumn fx:id="phoneNumberColumn" minWidth="20.0" prefWidth="111.0" text="PhoneNumber" />
                <TableColumn fx:id="stockColumn" minWidth="0.0" prefWidth="106.0" text=" State" />
                <TableColumn fx:id="addressColumn" minWidth="0.0" prefWidth="143.0" text="Adress" />
            </columns>
        </TableView>
      <Button fx:id="infor" layoutX="308.0" layoutY="517.0" mnemonicParsing="false" onAction="#BackToUser" prefHeight="31.0" prefWidth="113.0" styleClass="search-button" stylesheets="@../login.css" text="Back" />
    </children>

</AnchorPane>
